{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Properties": {
      "Application": "PFM.Api.v2"
    }
  },
  "AllowedHosts": "*",
  "AuthApi": {
    "EndpointUrl": "http://localhost:5000"
  }
}
